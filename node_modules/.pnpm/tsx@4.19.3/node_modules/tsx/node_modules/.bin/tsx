#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/dist/node_modules:/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/node_modules:/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/tsx@4.19.3/node_modules:/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/dist/node_modules:/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/tsx@4.19.3/node_modules/tsx/node_modules:/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/tsx@4.19.3/node_modules:/home/umohsamuel/Documents/GitHub/node-cli-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../dist/cli.mjs" "$@"
fi
